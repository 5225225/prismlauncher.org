<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ metadata.title }} - {{ eleventyNavigation.key or title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}"> {% favicon './img/icon.svg' %}
  </head>
  <body>
    <header>
      {#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}
      <nav id="nav">
        <div class="nav left">
          <a href="{{ '/' | url }}" class="home">
            <img src="{{ "/img/icon.svg" | url }}"> {{ metadata.title }}
          </a>
          <ul>
            {%- for entry in collections.all | eleventyNavigation %}
              <li class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %}">
                <a href="{{ entry.url | url }}">{{ entry.title }}</a>
              </li>
            {%- endfor %}
          </ul>
        </div>
        <div class="nav right">
          <ul>
            <li class="nav-item"><a>Download</a></li>
            <li class="nav-item"><a>Github</a></li>
          </ul>
      </nav>
    </body>
  </body>
</html></header>{{ content | safe }}<footer></footer><!-- Current page: {{ page.url | url }} --></body></html>