    <div class="hero-body">
      <div class="row container">
        <div class="column">
          <div class="hero-description">
            <h1 class="hero-title">{{ hero.title }}</h1>
            <p class="subtitle">{{ hero.description }}</p>
            {%- if hero.download == true %}
              <br>
                <a class="button is-big" href="{{ '/download' | url }}">Download Now</a>
              {%- endif %}
          </div>
          </div class="image">
          {%- if hero.image %}
            <div class="column">
              {% image hero.title, hero.image.light, hero.image.dark %}
            </div>
          {%- endif %}
        </div>
      </div>
    </div>
    {% endif %}
