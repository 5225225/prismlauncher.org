<nav class="container">
    <ul class="navbar">
        <li class="nav-item">
            <a href="{{ '/' | url }}" class="home">
                <img src="{{ "/img/icon.svg" | url }}" alt="Logo">
                    {{ metadata.title }}
                </a>
            </li>
            {%- for entry in collections.all | eleventyNavigation %}
                <li class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %}">
                    <a href="{{ entry.url | url }}">{{ entry.title }}</a>
                </li>
            {%- endfor %}
        </ul>
        <ul class="links">
            <li>
                <a class="button" href="{{ '/download/' | url }}">Download</a>
            </li>
            <li>
                <a class="github button" href="https://github.com/PolyMC/PolyMC">Github</a>
            </li>
        </ul>
    </ul>
</nav>
